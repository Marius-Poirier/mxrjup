{
  "name": "personal-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"node server/server.js\"",
    "start:backend": "node server/server.js",
    "start:frontend": "ng serve",
    "dev:computer": "cd computer-app && npm run dev",
    "build:angular": "ng build",
    "build:computer": "cd computer-app && npm run build",
    "build": "npm run build:angular && npm run build:computer",
    "postinstall": "cd server && npm install && cd ../computer-app && npm install",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.3",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@types/three": "^0.182.0",
    "rxjs": "~7.8.0",
    "three": "^0.182.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}